builders:
  object:
    auto_apply: all_packages
    import: "package:build_native/build_native.dart"
    builder_factories:
      - objectFileBuilder
    runs_before:
      - build_native|link
    build_extensions:
      .c:
        - .o
        - .obj
      .cc:
        - .o
        - .obj
      .cpp:
        - .o
        - .obj